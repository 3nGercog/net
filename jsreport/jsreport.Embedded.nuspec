<?xml version="1.0"?>
<package xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <id>jsreport.Embedded</id>
    <title>jsreport embedded server</title>
    <authors>Jan Blaha</authors>
    <version>0.0.0.0</version>
    <owners>Jan Blaha</owners>
    <description>Installs jsreport server into solution and allows to run it in parallel with .net process.</description>
    <projectUrl>http://jsreport.net</projectUrl>
    <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
    <iconUrl>http://jsreport.net/img/favicon.ico</iconUrl>
    <copyright>Copyright 2013-2014 Jan Blaha</copyright>
    <language>en-us</language>
    <tags>jsreport report pdf</tags>
    <dependencies>      
      <dependency id="DotNetZip" version="1.9.3"/>
      <dependency id="Microsoft.Bcl.Async" version="1.0.168"/>
      <dependency id="jsreport.Client" version="0.1.1"/>
      <dependency id="Simple.OData.Client" version="3.0.2"/>
    </dependencies>
  </metadata>
  <files>
    <file src="jsreport.Embedded\bin\Release\node.exe" target="content\node.exe" />
    <file src="jsreport.Embedded\bin\Release\jsreport-net-embedded.zip" target="content\jsreport-net-embedded.zip" />    
    <file src="jsreport.Embedded\bin\Release\jsreport.Embedded.dll" target="lib\net45\jsreport.Embedded.dll" />
    <file src="jsreport.Embedded\bin\Debug\jsreport.Embedded.pdb" target="lib\net45\jsreport.Embedded.pdb" />
    <file src="jsreport.Embedded\bin\Debug\jsreport.Embedded.xml" target="lib\net45\jsreport.Embedded.xml" />
    <file src="jsreport.Embedded\bin\Release\install.ps1" target="tools\install.ps1" />
  </files>
</package>
