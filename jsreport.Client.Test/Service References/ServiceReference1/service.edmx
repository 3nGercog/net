<edmx:Edmx xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx" Version="1.0">
  <edmx:DataServices xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata" m:DataServiceVersion="1.0" m:MaxDataServiceVersion="2.0">
    <Schema Namespace="$entity" xmlns="http://schemas.microsoft.com/ado/2008/09/edm">
      <EntityType Name="Template">
        <Key>
          <PropertyRef Name="_id" />
        </Key>
        <Property xmlns:p6="http://schemas.microsoft.com/ado/2009/02/edm/annotation" Name="_id" Nullable="false" p6:StoreGeneratedPattern="Identity" Type="Edm.String" />
        <Property Name="shortid" Type="Edm.String" />
        <Property Name="version" Type="Edm.String" />
        <Property Name="name" Type="Edm.String" />
        <Property Name="generatedReportsCounter" Type="Edm.Int32" />
        <Property Name="html" Type="Edm.String" />
        <Property Name="recipe" Type="Edm.String" />
        <Property Name="helpers" Type="Edm.String" />
        <Property Name="engine" Type="Edm.String" />
        <Property Name="modificationDate" Type="Edm.DateTime" />
        <Property Name="isExample" Type="Edm.Boolean" />
        <Property Name="script" Type="$entity.Script" />
        <Property Name="dataItem" Type="$entity.DataItem" />
        <Property Name="images" Type="Collection($entity.ImageRef)" />
      </EntityType>
      <EntityType Name="TemplateVersion">
        <Key>
          <PropertyRef Name="_id" />
        </Key>
        <Property xmlns:p6="http://schemas.microsoft.com/ado/2009/02/edm/annotation" Name="_id" Nullable="false" p6:StoreGeneratedPattern="Identity" Type="Edm.String" />
        <Property Name="shortid" Type="Edm.String" />
        <Property Name="lastVersion" Type="Edm.Int32" />
      </EntityType>
      <EntityType Name="Image">
        <Key>
          <PropertyRef Name="_id" />
        </Key>
        <Property xmlns:p6="http://schemas.microsoft.com/ado/2009/02/edm/annotation" Name="_id" Nullable="false" p6:StoreGeneratedPattern="Identity" Type="Edm.String" />
        <Property Name="shortid" Type="Edm.String" />
        <Property Name="name" Type="Edm.String" />
        <Property Name="content" Type="Edm.Binary" />
        <Property Name="contentType" Type="Edm.String" />
      </EntityType>
      <EntityType Name="Setting">
        <Key>
          <PropertyRef Name="_id" />
        </Key>
        <Property xmlns:p6="http://schemas.microsoft.com/ado/2009/02/edm/annotation" Name="_id" Nullable="false" p6:StoreGeneratedPattern="Identity" Type="Edm.String" />
        <Property Name="key" Type="Edm.String" />
        <Property Name="value" Type="Edm.String" />
      </EntityType>
      <ComplexType Name="Script">
        <Property Name="content" Type="Edm.String" />
      </ComplexType>
      <ComplexType Name="DataItem">
        <Property Name="dataJson" Type="Edm.String" />
        <Property Name="name" Type="Edm.String" />
      </ComplexType>
      <ComplexType Name="ImageRef">
        <Property Name="shortid" Type="Edm.String" />
        <Property Name="name" Type="Edm.String" />
        <Property Name="imageId" Type="Edm.String" />
      </ComplexType>
      <EntityContainer m:IsDefaultEntityContainer="true" Name="Context">
        <EntitySet EntityType="$entity.Template" Name="templates" />
        <EntitySet EntityType="$entity.TemplateVersion" Name="templateVersions" />
        <EntitySet EntityType="$entity.Image" Name="images" />
        <EntitySet EntityType="$entity.Setting" Name="settings" />
      </EntityContainer>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>